{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "src_files",
						"type": "DatasetReference",
						"parameters": {
							"srccontainer": {
								"value": "@pipeline().parameters.src_container",
								"type": "Expression"
							},
							"srcfolder": {
								"value": "@pipeline().parameters.src_folder",
								"type": "Expression"
							},
							"srcfilename": {
								"value": "@pipeline().parameters.src_filename",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "src_target",
						"type": "DatasetReference",
						"parameters": {
							"destinationcontainer": {
								"value": "@pipeline().parameters.des_container",
								"type": "Expression"
							},
							"destinationfolder": {
								"value": "@pipeline().parameters.des_folder",
								"type": "Expression"
							},
							"destinationfile": {
								"value": "@pipeline().parameters.des_filename",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"src_container": {
				"type": "string",
				"defaultValue": "stagging"
			},
			"src_folder": {
				"type": "string",
				"defaultValue": "employees"
			},
			"src_filename": {
				"type": "string",
				"defaultValue": "employees.csv"
			},
			"des_container": {
				"type": "string",
				"defaultValue": "raw"
			},
			"des_folder": {
				"type": "string",
				"defaultValue": "employees"
			},
			"des_filename": {
				"type": "string",
				"defaultValue": "employees.csv"
			}
		},
		"folder": {
			"name": "Execute_full_load"
		},
		"annotations": []
	}
}